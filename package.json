{
  "name": "poa",
  "version": "6.0.0",
  "license": "MIT",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "prepare": "npm run clean && npm run test && npm run build",
    "watch": "npm run clean && tsc -w",
    "clean": "rimraf core coverage i18n router satcheljs state utils index.js index.d.ts index.js.map yarn-error.log",
    "test": "run-s types lint codestyle jest",
    "test:watch": "jest --watchAll --coverage false",
    "jest": "jest",
    "types": "tsc -p tsconfig.test.json --noEmit",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "codestyle": "prettier --list-different *.ts *.tsx *.json *.md '**/*.ts' '**/*.tsx' '**/*.json'",
    "fmt": "prettier --write *.ts *.tsx *.json *.md '**/*.ts' '**/*.tsx' '**/*.json'",
    "build": "tsc",
    "release:beta": "ts-node scripts/release-beta.ts"
  },
  "dependencies": {
    "@types/history": "4.6.2",
    "@types/i18next": "8.4.3",
    "history": "4.7.2",
    "i18next": "11.3.5",
    "mobx": "4.3.1",
    "mobx-little-router": "2.0.0-4",
    "mobx-little-router-react": "2.0.0-4",
    "mobx-react": "5.2.3",
    "raven-js": "^3.26.3",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@types/jest": "23.1.4",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/react-test-renderer": "16.0.1",
    "axios": "^0.18.0",
    "jest": "23.3.0",
    "npm-run-all": "4.1.3",
    "nscript": "^0.1.10",
    "prettier": "1.13.7",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-test-renderer": "16.4.1",
    "rimraf": "2.6.2",
    "ts-jest": "23.0.0",
    "ts-node": "^7.0.1",
    "tslint": "5.10.0",
    "tslint-config-prettier": "1.13.0",
    "tslint-eslint-rules": "5.3.1",
    "tslint-react": "3.6.0",
    "typescript": "3.0.1"
  },
  "peerDependencies": {
    "react": "x.x.x",
    "react-dom": "x.x.x"
  }
}
