    "@poa/state": "5.0.2",
    "@types/i18next": "8.4.3",
    "@types/react": "16.3.16",
    "@types/react-dom": "16.0.5",
    "history": "4.7.2",
    "mobx": "4.3.0",
    "mobx-little-router": "1.7.4",
    "mobx-little-router-react": "1.7.5",
